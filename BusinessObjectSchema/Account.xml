<?xml version="1.0" encoding="utf-8"?>
<businessObject>
	<propertyGroup>
		<property name="Id" type="int" />
		<property name="Name" type="string" />
		<property name="DateCreated" type="System.DateTime" />
	</propertyGroup>
	<script>
		CREATE TABLE Account (
			Id INTEGER PRIMARY KEY,
			Name TEXT,
			DateCreated TEXT
		);
	</script>
</businessObject>